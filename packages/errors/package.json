{
  "name": "@jsonql/errors",
  "version": "0.7.0",
  "description": "Jsonql Errors class",
  "main": "dist/index.js",
  "module": "./src/index",
  "files": [
    "dist",
    "src",
    "index.js",
    "index.d.ts"
  ],
  "scripts": {
    "test:ava": "ava",
    "test:nyc": "nyc ava",
    "test": "pnpm build && pnpm test:nyc",
    "build:tsc": "tsc -p tsconfig.json",
    "build": "pnpm clean:dist && pnpm build:tsc",
    "clean:dist": "rimraf ./dist",
    "updateDeps": "node ../../update-deps.js errors",
    "prepublishOnly": "pnpm build && pnpm test",
    "postpublish": "pnpm updateDeps",
    "ts": "node -r esbuild-register",
    "build:swc": "swc ./src -d ./js"
  },
  "keywords": [
    "node.js",
    "jsonql",
    "errors"
  ],
  "author": "Joel Chu <joelchu@foxmail.com>",
  "homepage": "https://jsonql.org",
  "license": "MIT",
  "ava": {
    "verbose": true,
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "esbuild-register"
    ]
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.7",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "superkoa": "^1.0.3"
  }
}
