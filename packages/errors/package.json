{
  "name": "@jsonql/errors",
  "version": "0.2.0",
  "description": "Jsonql Errors class",
  "main": "index.js",
  "module": "./src/index.mjs",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test:ava": "ava",
    "test": "nyc ava",
    "postpublish": "pnpm updateDeps",
    "updateDeps": "node ../../update-deps.js errors"
  },
  "keywords": [
    "node.js",
    "jsonql",
    "errors"
  ],
  "author": "Joel Chu <joelchu@foxmail.com>",
  "homepage": "https://jsonql.org",
  "license": "MIT",
  "dependencies": {
    "@jsonql/constants": "workspace:^0.2.0"
  },
  "ava": {
    "files": [
      "tests/*.test.js"
    ],
    "ignoredByWatcher": [
      "tests/helpers/*.*",
      "tests/fixtures/*.*"
    ],
    "cache": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "verbose": true,
    "require": [
      "esbuild-register"
    ]
  },
  "devDependencies": {
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "superkoa": "^1.0.3"
  }
}
